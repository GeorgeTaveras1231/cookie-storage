  "main": "./dist/cookie-storage.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bouzuya/cookie-storage.git"
  },
  "scripts": {
    "build": "npm-run-all -s 'build:ts' 'build:es2015' -p 'copy:*'",
    "build:es2015": "babel --out-dir .tmp/es5/ .tmp/es2015/",
    "build:ts": "tsc",
    "clean": "rimraf .tmp dist",
    "copy:dts": "cpx '.tmp/es2015/src/**/*.d.ts' 'dist/'",
    "copy:js": "cpx '.tmp/es5/src/**/*.js' 'dist/'",
    "test": "gulp test",
    "watch": "gulp watch"
