    "coffee-script": "^1.9.1",
    "cpx": "1.4.0",
    "gulp": "^3.8.11",
    "gulp-babel": "^4.0.0",
    "gulp-coffee": "^2.3.1",
    "gulp-espower": "^0.10.1",
    "gulp-mocha": "^2.0.0",
    "gulp-sourcemaps": "^1.5.1",
    "gulp-util": "^3.0.4",
    "npm-run-all": "3.1.0",
    "power-assert": "^0.10.2",
    "rimraf": "2.5.4",
    "typescript": "2.0.2"
  },
  "homepage": "https://github.com/bouzuya/cookie-storage",
  "keywords": [
    "browser",
    "cookie",
    "storage"
  ],
